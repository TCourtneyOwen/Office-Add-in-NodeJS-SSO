{
  "name": "ssoapp",
  "version": "0.0.0",
  "private": true,
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop"
  },
  "scripts": {
    "build": "npm-run-all clean -p tsc cpx",
    "clean": "rimraf dist",
    "configure-sso": "node ./node_modules/office-addin-sso/lib/cli.js configure manifest.xml Office-Addin-NodeJS-SSO",
    "cpx": "cpx \"./src/**/!(*.js)\" dist && cpx \"assets/images/**/*.*\" dist/images && cpx \"src/index.html\" dist && cpx \"styles/**/*.*\" dist/styles && cpx \"views/**/*.*\" dist/views",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sideload": "office-addin-debugging start manifest.xml",
    "start": "concurrently \"npm run start-server\" \"npm run sideload\" ",
    "start-server": "tsc && node src/bin/www"
  },
  "dependencies": {
    "@types/jquery": "^3.3.31",
    "@types/office-js": "^1.0.29",
    "cookie-parser": "~1.4.4",
    "cpx": "^1.5.0",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "envfile": "^3.0.0",
    "express": "~4.16.1",
    "form-urlencoded": "^4.0.0",
    "http-errors": "~1.6.3",
    "jQuery": "^1.7.4",
    "jshint": "^2.10.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "node-fetch": "^2.6.0",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "office-addin-debugging": "^3.0.16",
    "office-addin-dev-certs": "^1.4.11",
    "office-addin-manifest": "^1.4.12",
    "concurrently": "^4.1.2",
    "generate-password": "^1.4.2",
    "rimraf": "3.0.0",
    "typescript": "^3.6.4"
  }
}
