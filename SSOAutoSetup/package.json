{
  "name": "ssoapp",
  "version": "0.0.0",
  "private": true,
  "config": {
    "app-to-debug": "excel",
    "app-type-to-debug": "desktop"
  },
  "scripts": {
    "build": "npm-run-all clean -p tsc cpx",
    "clean": "rimraf dist",
    "configure-sso": "node ./node_modules/office-addin-sso/lib/cli.js configure manifest.xml Office-Addin-NodeJS-SSO",
    "cpx": "cpx \"images/**/*.*\" dist/images && cpx \"src/**/*.*\" dist/src && cpx \"src/taskpane.html\" dist && cpx \"src/fallbackAuthDialog.html\" dist && cpx \"styles/**/*.*\" dist/styles && cpx \"src/views**/*.*\" dist",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "sideload": "office-addin-debugging start manifest.xml",
    "start": "concurrently \"npm run start-server\" \"npm run sideload\" ",
    "start-server": "node ./node_modules/office-addin-sso/lib/cli.js start manifest.xml"
  },
  "dependencies": {
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.12.5",
    "@types/office-js": "^1.0.29",
    "cookie-parser": "~1.4.4",
    "cpx": "^1.5.0",
    "dotenv": "^8.1.0",
    "jQuery": "^1.7.4",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "office-addin-debugging": "^3.0.16",
    "office-addin-dev-certs": "^1.4.11",
    "office-addin-manifest": "^1.4.12",
    "concurrently": "^4.1.2",
    "generate-password": "^1.4.2",
    "rimraf": "3.0.0",
    "typescript": "^3.6.4"
  }
}
